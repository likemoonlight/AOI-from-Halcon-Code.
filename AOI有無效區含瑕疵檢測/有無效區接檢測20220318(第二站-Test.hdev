<l>    endif</l>
<c>    */////////////////////////////////////////////////</c>
<l>    if(White_4000_num&gt;0)</l>
<c>*/////////////////////Black檢測//////////////////////</c>
<c>*/////////////////////檢測結束//////////////////////        </c>
<l>    endif</l>
<c>    */////////////////////////////////////////////////</c>
<l>  endfor </l>
